<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Publish_End_Date_Later_Than_Start</fullName>
    <active>true</active>
    <errorConditionFormula>Publish_end_date__c  &gt;  Publish_start_date__c</errorConditionFormula>
    <errorDisplayField>Publish_end_date__c</errorDisplayField>
    <errorMessage>Publish End Date cannot be earlier than Publish Start Date</errorMessage>
</ValidationRule>
